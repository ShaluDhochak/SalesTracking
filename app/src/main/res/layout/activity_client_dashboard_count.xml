<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.ClientDashboardCountActivity">


    <RelativeLayout
        android:id="@+id/headerClientDashboard_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <include
            layout="@layout/header_layout"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/clientDashboardDetail_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/headerClientDashboard_rl"
        >
        <include
            layout="@layout/client_dashboard_layout"/>
    </RelativeLayout>

</RelativeLayout>